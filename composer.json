{
  "name": "madewithlove/skeletor",
  "description": "The Laravel Framework.",
  "keywords": [
	"framework",
	"laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
	"laravel/framework": "4.2.*",
    "rcrowe/twigbridge": "0.6.*",
    "anahkiasen/arrounded": "0.5.*",
    "bugsnag/bugsnag-laravel": "~1.3"
  },
  "require-dev": {
    "fabpot/php-cs-fixer": "~1.4",
    "way/generators": "~2.6",
    "codeception/codeception": "~2.0",
    "barryvdh/laravel-ide-helper": "~1.11",
    "doctrine/dbal": "~2.5",
    "barryvdh/laravel-debugbar": "~1.8",
    "symfony/var-dumper": "~2.6",
    "phpspec/phpspec": "~2.1",
    "benconstable/phpspec-laravel": "~1.6"
  },
  "autoload": {
	"classmap": [
	  "app/database/migrations",
	  "app/database/seeds"
	],
	"psr-4": {
	  "App\\": "app/App"
	}
  },
  "repositories": [
	{
	  "type": "composer",
	  "url": "http://toran.mwlhq.com/repo/packagist/"
	},
	{
	  "packagist": false
	}
  ],
  "scripts": {
	"post-install-cmd": [
	  "php artisan clear-compiled",
	  "php artisan optimize"
	],
	"post-update-cmd": [
	  "php artisan clear-compiled",
	  "php artisan optimize"
	],
	"post-create-project-cmd": [
	  "mkdir -p app/App/Models",
	  "mv app/models/User.php app/App/Models/User.php",
	  "rm -rf app/tests/ExampleTest.php app/database/seeds/.gitkeep CONTRIBUTING.md app/controllers app/tests/* app/commands app/models app/views/hello.php",
	  "vendor/bin/codecept bootstrap app",
	  "mv app/codeception.yml ./codeception.yml",
	  "php artisan key:generate",
	  "curl -L http://bit.ly/vaprobash > Vagrantfile",
	  "git init",
	  "vendor/bin/php-cs-fixer fix app --fixers='-indentation'"
	]
  },
  "config": {
	"preferred-install": "dist"
  },
  "minimum-stability": "stable"
}
